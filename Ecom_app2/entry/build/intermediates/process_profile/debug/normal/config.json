{
    "app": {
        "apiVersion": {
            "compatible": 7,
            "releaseType": "Beta1",
            "target": 7
        },
        "vendor": "example",
        "bundleName": "com.example.ecom_app.hmservice",
        "version": {
            "code": 1000000,
            "name": "1.0.0"
        }
    },
    "deviceConfig": {
        "default": {
            "debug": true
        }
    },
    "module": {
        "deviceType": [
            "phone",
            "tablet",
            "tv",
            "wearable"
        ],
        "abilities": [
            {
                "skills": [
                    {
                        "entities": [
                            "entity.system.home"
                        ],
                        "actions": [
                            "action.system.home"
                        ]
                    }
                ],
                "visible": true,
                "name": "com.example.ecom_app.MainAbility",
                "icon": "$media:icon",
                "description": "$string:mainability_description",
                "label": "$string:entry_MainAbility",
                "type": "page",
                "launchType": "standard",
                "homeAbility": true
            }
        ],
        "mainAbility": "com.example.ecom_app.MainAbility",
        "distro": {
            "moduleType": "entry",
            "installationFree": true,
            "deliveryWithInstall": true,
            "moduleName": "entry"
        },
        "package": "com.example.ecom_app",
        "name": ".MyApplication",
        "js": [
            {
                "pages": [
                    "pages/index/index",
                    "pages/detail/detail",
                    "pages/filter/filter"
                ],
                "name": "default",
                "window": {
                    "designWidth": 720,
                    "autoDesignWidth": true
                }
            }
        ]
    }
}